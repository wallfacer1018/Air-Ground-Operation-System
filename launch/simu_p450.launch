<launch>
    <!-- world -->
    <include file="$(find easondrone_gazebo)/launch/empty_world.launch"/>

    <!-- 1号无人机 -->
    <include file="$(find easondrone_gazebo)/launch/sitl_p450.launch"/>

    <include file="$(find easondrone_gazebo)/launch/transform.launch"/>

    <include file="$(find cuadc)/launch/control_p450_indoor.launch"/>

    <include file="$(find cuadc)/launch/visual_infer.launch"/>
<!--    <include file="$(find cuadc)/launch/darknet_ros.launch"/>-->
<!--    <include file="$(find cuadc)/launch/find_object_2d.launch"/>-->

    <node pkg="cuadc" type="cuadc_node" name="cuadc_node" launch-prefix="gnome-terminal --"/>

    <!-- 启动rviz -->
    <node type="rviz" name="rviz" pkg="rviz" args="-d $(find cuadc)/config/CUADC.rviz"/>

    <node type="rqt_image_view" name="rqt_image_view" pkg="rqt_image_view" args="/visual_infer/image_raw"/>
</launch>
